{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf02-nyc-Taxi"
		},
		"adls_nyc_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_nyc'"
		},
		"adls_nyc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://nyctaxi02storage.dfs.core.windows.net/"
		},
		"nyc_web_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://d37ci6vzurychx.cloudfront.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf02-nyc-Taxi_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_nyc_accountKey": {
						"value": "[parameters('adls_nyc_accountKey')]"
					},
					"adls_nyc_properties_typeProperties_url": {
						"value": "[parameters('adls_nyc_properties_typeProperties_url')]"
					},
					"nyc_web_properties_typeProperties_url": {
						"value": "[parameters('nyc_web_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}